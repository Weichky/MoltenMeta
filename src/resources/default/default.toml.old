[app]
name = "MoltenMeta"
version = "0.1.0105"
stage = "dev" # dev, alpha, beta, rc, stable
is_default_config = true

[runtime]
python_version = "3.14.2"
profile = "debug"

[database]
db_type = "sqlite"
sqlite_path = "data/db/moltenmeta.mmdb"

[log]
level = "info" # debug, info, warning, error, critical
enable_file_logging = false
enable_console = false

[locale]
language = "en"
theme = "blue"
scheme = "dark"